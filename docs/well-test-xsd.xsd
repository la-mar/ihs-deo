    <xsd:group name="IP_PT_BASE">
        <xsd:annotation>
            <xsd:documentation>Information about initial potential tests/production tests.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="HEADER">
                <xsd:complexType>
                    <xsd:group ref="TEST_HEADER"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="PERFORATION" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="TREATMENT" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="PRESSURE" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about pressures during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="HEADER">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the top of
                                                            the tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the base
                                                            of the texted interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DATUM_POOL" type="ihse_depth" minOccurs="0"/>
                                                <xsd:element name="DATUM_WELL" type="ihse_depth" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Pressures during the production test.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="FLOWING" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The tubing pressure measured
                                                                        under flowing conditions.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The casing pressure measured
                                                                        under flowing conditions (the pressure built up
                                                                        between the casing and the tubing).
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="SHUTIN" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The shutin pressure measured in
                                                                        the tubing
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The casing pressure measured
                                                                        under shutin conditions.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Rock units in the tested interval.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Stratigraphic unit present at the top of the
                                                            tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Stratigraphic unit present at the base of the
                                                            tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="BOTTOM_HOLE" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Bottom hole conditions during the test.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depths associated with the bottomhole pressure test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="PACKER" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The depth the packer equipment was set to
                                                            seal the casing or tubing.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="RUN" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The depth at which the bottom hole pressure
                                                            reading was taken by the recorder.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="SEA_LEVEL_DATUM" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The sea level reference datum. This is
                                                            calculated by taking elevation and subtracting the recorder
                                                            depth. The value has a (-) for below sea level.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole pressure during the test.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="FLOWING" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="WELLHEAD" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="DATUM" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="GRADIENT" type="ihse_pressure_gradient" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Time duration required to conduct the bottom hole
                                                pressure test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole temperature during the bottom hole pressure
                                                test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TUBING_SIZE" type="ihse_length" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description of the tubing size used during the bottom
                                                hole pressure test
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ABSOLUTE_OPEN_FLOW" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Absolute open flow well pressure information pertaining to the
                                    production of oil or gas under wide-open conditions (the flow of production from a
                                    well without any restrictions on the rate of flow).
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="HEADER">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                                                <xsd:element name="FLOWS" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Open flows.</xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="CALCULATED" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Calculated open flow potential.
                                                                        The theoretical rate that a well would flow at
                                                                        the wall of the well with zero pounds pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FOUR_POINT" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The calculated absolute open flow
                                                                        based on a 4 point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="PRESSURES" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Pressures associated with the absolute open
                                                            flow pressure.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="SHUTIN" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The instantaneous shut-in
                                                                        pressure measured in a four point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The average reservoir pressure.
                                                                        This is the shut-in pressure for a four point
                                                                        test and referred to as formation/reservoir
                                                                        pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="INVERSE" type="xsd:double" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The slope of the stabilized deliverability
                                                            line. The points of the 4pt test are plotted and the slope
                                                            determines optimum flow rate.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="FOUR_POINT" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about a four point test conducted in the
                                                well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                <xsd:element name="CHOKE" type="ihse_size" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Choke size during the four point test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FLOW" type="ihse_fluid" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The stablized flow rate for the test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="PRESSURES" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Pressures during the four point test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="FLOW" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The sandface pressure at the
                                                                        stabilized flow rate.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="BOTTOM_HOLE" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The bottom hole pressure during
                                                                        the four point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The duration of the absolute open flow
                                                            period.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_WATER" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of water produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type of fluid being analyzed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit present at the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit present at the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TEMPERATURE"  minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RESERVOIR"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="UOM_ORIGINAL" type="xsd:string"  minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="RW"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:double"  minOccurs="0"></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string"  minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                        <xsd:element name="RESISTIVITY" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="VALUE" type="xsd:double" minOccurs="0">
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                        <xsd:element name="SALINITY" type="ihse_salinity_water_analysis" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The total weight of dissolved salts in the water sample.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TYPE_NAME" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="VALUE" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="DESCRIPTION" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="LABORATORY_REFERENCE" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_OIL" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of oil produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="RATIOS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="GAS_OIL" type="ihse_ratio" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units present in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the top of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the bottom of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="VISCOSITY" minOccurs="0" >
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:int" minOccurs="0" ></xsd:element>
                                    <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                    <xsd:element name="UOM_ORIGINAL" type="xsd:string" minOccurs="0" ></xsd:element>
                                    <xsd:element name="UOM" type="xsd:string"  minOccurs="0"  ></xsd:element>
                                    <xsd:element name="TYPE" type="xsd:string"  minOccurs="0"  ></xsd:element>
                                    <xsd:element name="POUR_POINT" minOccurs="0" >
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string" minOccurs="0" ></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TEMPERATURE" minOccurs="0" >
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string" minOccurs="0" ></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_GAS" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of gas produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units present in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the top of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the base of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                            <xsd:element name="CARBON_DIOXIDE" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of carbon dioxide in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HEAT_CONTENT" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Heat content of the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="GRAVITY" type="ihse_gravity" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Gravity of the gas produced during the test.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HELIUM" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of helium in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="NITROGEN" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of nitrogen in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HYDROGEN_SULFIDE" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of hydrogen sulfide in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="FLUID" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TYPE" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0"/>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="FIELD_DATA" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about the field where the well is located.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="DISCOVERY_WELL" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Y(es) or N(o) indicating the well is a discovery well. A discovery is
                                    an exploratory well that has penetrated a new oil/gas producing reservoir or field.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="FIELD" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Name of the field where the well is located.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SUB_HORIZON" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Number identifying each producing interval in a well in chronological
                                    order (e.g., 1 = original producing interval, 2 = deepened producing
                                    interval, etc.).
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="OPERATORS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Organizations that operated the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CURRENT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The current operator of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ORIGINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The original operator of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TOP_PAY" type="ihse_depth" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Measured depth from the surface to the top of the pay zone.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="LEASE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Name assigned to the lease by the regulatory agency with jurisdiction
                                    over the minerals in the area where the lease is located.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="SHUTOFF" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about the shutoff used in the wellbore.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type of shutoff used in the wellbore (e.g., bridge
                                    plug, cased off, plugged off, or squeezed, etc.).
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the shutoff interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the shutoff
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the
                                                shutoff interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PLUGBACK" type="ihse_distance" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the cement plug.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Narrative remarks about the initial potential/production tests.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Narrative remarks about the initial potential/production tests.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>