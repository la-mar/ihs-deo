<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2008 rel. 2 sp2 (http://www.altova.com) by Raj Sehgal (INFORMATION HANDLING SERVICES INC) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified">
    <xsd:include schemaLocation="EnerdeqML_v1.19_Types.xsd"/>
    <xsd:element name="WELL_SET">
        <xsd:annotation>
            <xsd:documentation>A collection of wellbores</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="METADATA" type="ihse_set_metadata"/>
                <xsd:element ref="WELLBORE" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="WELLBORE">
        <xsd:annotation>
            <xsd:documentation>The hole made by the bit.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:group ref="WELLBORE_DATA"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:group name="WELLBORE_DATA">
        <xsd:sequence>
            <xsd:group ref="HEADER_WELL"/>
            <xsd:element ref="PRIOR_INFORMATION" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="LOCATION" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="RESERVOIR" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="SOURCE" type="xsd:string"/>
                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                        <xsd:element name="POOL" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="ZONE" type="ihse_name_code" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="DRILLING" minOccurs="0"/>
            <xsd:element ref="OWNERSHIP" minOccurs="0"/>
            <xsd:element ref="OPERATION" minOccurs="0"/>
            <xsd:element ref="SURVEYS" minOccurs="0"/>
            <xsd:element ref="MECHANICAL" minOccurs="0"/>
            <xsd:element ref="GEOLOGY" minOccurs="0"/>
            <xsd:element ref="GEOPHYSICS" minOccurs="0"/>
            <xsd:element ref="ENGINEERING" minOccurs="0"/>
            <xsd:element name="TESTS" minOccurs="0">
                <xsd:complexType>
                    <xsd:group ref="WELL_TESTS"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Remarks that apply to the entire well.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="SOURCE" type="xsd:string"/>
                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>General well remark.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="STAGE" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="USER_DATA" type="ihse_generic" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="CONTENT" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="STAGE" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="PRIOR_INFORMATION" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="WELL_NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="LEASE" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="OPERATOR" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="IDENTIFICATION" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="LOCATION" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="RESERVOIR" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="DRILLING" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="BIT" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="FISH" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="BREAK" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PROBLEM" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="LOST_CIRCULATION" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="MEDIA" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PLUGBACK" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="SHOW" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="WHIPSTOCK" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="BLOWOUT" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="HORIZONTAL" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="NARRATIVE" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="OWNERSHIP" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="INTEREST" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="OPERATION" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="ABANDONMENT" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SURVEYS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="BOREHOLE" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="MECHANICAL" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CASING" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="LINER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TUBING" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="GEOLOGY" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CORE" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="STRATIGRAPHIC_UNIT" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="GEOLOGIST" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="LITHOLOGY" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="POROSITY" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PAYZONE" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="FAULT" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="SHALE" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="GEOPHYSICS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="LOG" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ENGINEERING" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="COMPLETION" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PERFORATION" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PRESSURE" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TESTS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOTAL" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="INITIAL_PRODUCTION" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="PRODUCTION" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="DRILL_STEM" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="UNSPECIFIED" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TREATMENT" type="xsd:long" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="NARRATIVE" type="xsd:long" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:group name="HEADER_WELL">
        <xsd:annotation>
            <xsd:documentation>Header information for the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="METADATA" type="ihse_entity_metadata"/>
            <xsd:element name="HEADER">
                <xsd:annotation>
                    <xsd:documentation>Header information for the well.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="SOURCE" type="xsd:string"/>
                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code">
                            <xsd:annotation>
                                <xsd:documentation>Actual owner of the data.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="IDENTIFICATION" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Number uniquely identifying a well within a producing entity.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:simpleContent>
                                    <xsd:extension base="xsd:string">
                                        <xsd:attribute name="TYPE" type="xsd:string" use="required"/>
                                    </xsd:extension>
                                </xsd:simpleContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="GEOPOLITICAL" type="ihse_geopolitical" minOccurs="0"/>
                        <xsd:element name="OPERATORS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Organizations that operated the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="ORIGINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Organization that owned the well at time of completion.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ORIGINAL_CITY" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>City location of the organization that operated the well
                                                at time of completion.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CURRENT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Organization that currently operates the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CURRENT_CITY" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>City location of the organization that currently operates
                                                the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>

                        </xsd:element>
                        <xsd:element name="RIGHTS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The full name of the entity and number assigned to the lease by the
                                    regulatory agency with the jurisdiction/rights over mineral activity on the lease.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="NUMBER" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Number assigned to the lease by the regulatory agency
                                                with the
                                                jurisdiction/rights over mineral activity on the lease.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of property lease ownership.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OILSAND_AREA" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The natural reservoir of oil sands.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DRILL_COST_AREA" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The drilling cost area defined in the exploratory well
                                                regulations.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="INCENTIVE_CERTIFICATION" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Name on the incentive certificate.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="AREAS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RIGHTS" type="ihse_area" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The surface area contained within the rights.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="UNIT" type="ihse_area" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The surface area contained within the unit that the
                                                rights is part of.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="NUMBER" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Number assigned to each well within the entity/lease by the
                                    operator.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Name given well.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="FIELD" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Legal or common given name of the field where the well is located.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:complexContent>
                                    <xsd:extension base="ihse_name_code">
                                        <xsd:sequence>
                                            <xsd:element name="ALIAS" type="xsd:string" minOccurs="0"/>
                                        </xsd:sequence>
                                    </xsd:extension>
                                </xsd:complexContent>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="BLOCK" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Block well is within</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="OWNER" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Company or group of companies owning rights to block.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OPERATOR" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Company operating block.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type and/or status of the production string.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="GEOLOGIC_PROVINCE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The distinct geographical area with similar geologic features for the
                                    well.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="BASIN" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Basins</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="MAIN" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Main basin</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="SUB_BASIN" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Sub-basin</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units in the wellbore.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="OBJECTIVE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The stratigraphic unit to which the well is contracted to
                                                be drilled.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OBJECTIVE_BOTTOM_HOLE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The proposed producing formation that is expected to be
                                                reached at the total depth of the well when drilled.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BOTTOM_HOLE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The stratigraphic unit at the total depth.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CONFIDENTIAL" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="PERMIT" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Information about the well permit.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>License number for the permit to drill.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LICENSEE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Comany or group receiving the permit.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FILER" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The authorized person who signs the application for the
                                                permit to drill.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PHONE" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The main phone number for the filer.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ADDRESS_FIRST" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description or text of the first line of the address for
                                                a business associate.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ADDRESS_SECOND" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description or text of second line of address for a
                                                business
                                                associate.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ADDRESS_THIRD" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description or text of third line of address for a
                                                business associate.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CITY" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The city in which the business associate is located.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PROVINCE_STATE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The state or political province.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="COUNTRY" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The division of land, political nation or territory.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="POSTAL_CODE" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Code or number assigned by the postal service identifying
                                                a mail delivery zone.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ADDRESS_TYPE" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of address. For example shipping, billing,
                                                sales, home etc.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="EMAIL" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The main Email address for the business associate.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FAX" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The main fax number for the business associate.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DAYS" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Number of days left until the Permit expires
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LOCATION_EXCEPTION" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Information for when an exception is given to where the
                                                well should have been drilled.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="IDENTIFICATION" type="xsd:string" minOccurs="0"/>
                                                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="REQUESTED" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>An indicator identifying that a location
                                                            exception was applied for. This exception is a request to
                                                            exceed the normal spacing rule for a offset well due to
                                                            natural obstructions (ie. rivers, lakes) or environmental
                                                            concerns.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="GRANTED" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>An indicator identifying that a location
                                                            exception was granted. This exception is a request to exceed
                                                            the normal spacing rule for a offset well due to natural
                                                            obstructions (ie. rivers, lakes) or environmental concerns.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Remarks or comments pertaining to the permit
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DRILLING" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Drilling information for the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CONTRACTOR" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The company that owns and operates the drilling rig.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RIG" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="NUMBER" type="xsd:string" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The number of the rig being used by the
                                                            drilling contractor.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="PERIOD" type="ihse_decimal" minOccurs="0"/>
                                                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="HEIGHT_FOUNDATION" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The height of the foundation on which the
                                                            derrick and engine sits.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TOOL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of drilling tools proposed to drill the well
                                                (e.g., rotary, cable, or a combination).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PLATFORM" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about the drilling platform.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Identification of platform.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="RIG_SITE" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The type of rig site to be used (e.g., barge,
                                                            drill
                                                            ship, platform, etc.).
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DRILL_SLOT" type="xsd:long" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Number for the proposed drill slot on the
                                                            offshore platform.
                                                            Platforms have 24 to 32 slots in which to position derricks
                                                            for drilling.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="COMPLETION_TYPE" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The type of completion associated with the
                                                            well.
                                                            For example, commingled, multiple completion, duel
                                                            completion,
                                                            etc.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DISTANCE_TO_REFERENCE" type="ihse_distance" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The distance from a reference to
                                                            the well location
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="LOCATION_REFERENCE" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The location or reference point from where
                                                            the distance to the offshore drilling rig was measured.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="HOLE_DIRECTION" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The direction of the borehole (vertical, directional,
                                                horizontal).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="SURVEY_FLAG" type="xsd:string" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Indicates if a directionral survey has been
                                                            run on the well.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="TVD_CALCULATION" type="xsd:string" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Indicates if a true vertical depth
                                                            calculation has been performed on the well.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="COSTS" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="PLANNED" type="ihse_cost" minOccurs="0"/>
                                                <xsd:element name="ACTUAL" type="ihse_cost" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Various significant depths in the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOTAL_MEASURED_OBJECTIVE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The depth to which the well was contracted to be
                                                drilled.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_TRUE_VERTICAL_OBJECTIVE" type="ihse_depth" minOccurs="0"/>
                                    <xsd:element name="TOTAL_DRILLER" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Total depth of the well as reported by the driller.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_LOGGER" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Total depth of the well as reported by the logger.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_TRUE_VERTICAL" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The vertical depth of the well, measured from the surface
                                                to the bottom of the hole.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_OLD" type="ihse_distance" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Total depth of the well before it was reworked, drilled
                                                deeper, or plugged back.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_MEASURED" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The total depth of the well, measured along the
                                                wellbore.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DEEPEST" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Deepest depth drilled in well, generally during a
                                                remedial operation.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PLUGBACK" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The depth to the plug in the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PACKER" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The depth to the packing device in the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WATER" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depth of the water at the well/platform location
                                                (measured from the
                                                water level to the mud line).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WATER_REFERENCE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Identifies the point near the well from where the water
                                                depths were measured (e.g., ground, mean sea level, mean tide level,
                                                mean high water level)
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TOTAL_DRILLED" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Total distance drilled. Thiis includes sidetracks and
                                                workovers.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="WHIPSTOCK" type="ihse_depth" minOccurs="0"/>
                                    <xsd:element name="CONFIDENTIAL" type="ihse_depth" minOccurs="0"/>
                                    <xsd:element name="KICKOFF" type="ihse_depth" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DATES" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Dates for various events relating to the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RIGHTS_EXPIRY" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Expiry date identifying when the lookup code was
                                                inactivated in the database
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PERMIT" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date the well license was issued.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PERMIT_EXPIRY" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date permit expires.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PERMIT_REISSUE" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date permit expires.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FIRST_SPUD" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>First spud date.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="SPUD" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The date drilling operations began on the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="COMPLETION" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date of the official filing or completion report
                                                indicating the well is established as ready to produce, inject or dry.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FIRST_SPUD_RIG_RELEASE" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>First spud rig release date.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="COMPLETION_REPORT" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>A report filed with the appropriate regulatory agency
                                                upon completion of the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FINAL_DRILLING" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The date the drilling operations for the well were
                                                finalized.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RIG_RELEASE" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date the drilling rig can be released from operations and
                                                moved from the well site as specified in the contract.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RIG_ON_SITE" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The date the drilling rig arrives on the well site.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DRILL_ABANDONMENT" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="SUSPENDED" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="ABANDONMENT" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date the well was permanently abandoned.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date the data was first entered into database.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date data for well was last changed or added.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PRODFIT_UPDATE_DATE" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="REGULATORY_WELL_STATUS"  type="ihse_date" minOccurs="0">
                                    </xsd:element>
                                    <xsd:element name="ON_PRODUCTION" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="ON_PRODUCTION_CALCULATED" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="ON_INJECTION" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The date injection activities began in the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ON_INJECTION_CALCULATED" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="CONFIDENTIAL_RELEASE" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="COST_ACTUAL" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="STATUS_PRODUCTION_CURRENT" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="INCENTIVE_CERTIFICATION" type="ihse_date" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Date on the incentive certificate.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ELEVATIONS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Elevation of various points relative to mean sea level.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="GROUND" type="ihse_elevation" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The ground elevation at the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="KELLY_BUSHING" type="ihse_elevation" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The elevation of the Kelly Bushing.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="REFERENCE" type="ihse_elevation" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The elevation reference from which depths were
                                                measured.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DERRICK_FLOOR" type="ihse_elevation" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The elevation of the derrick floor.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CASING_FLANGE" type="ihse_elevation" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The elevation of the casing flange in the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="PARENT" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Information about the well's parent well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RELATIONSHIP" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of relationship between the parent well and this
                                                well. For example, sidetrack, recompletion, deepening, re-drill,
                                                re-entry or surface.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="UWI" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>For complex wells with multiple wellbores, provides the
                                                relating parent's UWI.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="STATUSES" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Description of the well's status.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="PRODUCTION_CURRENT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description of the current status of the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PRODUCTION_PREVIOUS" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The previous reported status of the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PRODUCTION_ORIGINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The original status of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CURRENT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The current status of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The last reported event for a well [Examples: Suspended;
                                                Active].
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ACTIVITY" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Deontes generalized operational activity of the well (eg
                                                PERMIT, DRILLING, COMPLETED, ABANDONED, etc.)
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PLOT_SYMBOL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The character or symbol used for plotting the well
                                                status.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="REGULATORY_WELL_STATUS" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="CODE" type="xsd:string" minOccurs="0">
                                                </xsd:element>
                                                <xsd:element name="NAME" type="xsd:string" minOccurs="0">
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>

                                    <xsd:element name="MODE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Code identifying the operational mode of the well. The
                                                mode identifies what the well is doing. For example, flowing, pumping,
                                                abandoned, etc.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PERMIT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The Well License status identifies if the well permit is approved or unapproved by the government regulatory agency to
                                                drill a specific well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="CLASSES" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Classifications of the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="INITIAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Code identifying the initial Lahee classification of the
                                                well (the proposed well objective).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The final Lahee classification of the well (the
                                                reappraisal of the well objective after completion and testing).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="AGES" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="OBJECTIVE_YOUNGEST" type="ihse_age" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Age of yountest strata in the objective unit.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OBJECTIVE_OLDEST" type="ihse_age" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Age of the oldest strata in the objective unit.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="OLDEST_PENETRATED" type="ihse_age" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Age of oldest strata encountered in well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BOTTOM_HOLE" type="ihse_age" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Age of strata at total depth.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TAX_CREDIT" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Code indicating if the well qualifies for any tax credits.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SUB_COMPLETION" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Alphanumeric code tracking subcompletions recompletions) within the
                                    well.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="PRODUCTION_TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Manner is which the well is produced.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="UNIT_WELL_NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="PRESSURES" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="BOTTOM_HOLE" type="ihse_pressure" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole pressure.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TEMPERATURES" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="BOTTOM_HOLE" type="ihse_temperature" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole temperature.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="PERFORATIONS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths within the well from which production occurs.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth to the top of the shallowest producing
                                                interval in the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth to the top of the deepest producing
                                                interval in the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SITUATION" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Indicator of whether the well resides onshore or offshore.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TERRAIN" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The physiographic province in which the well is located such as
                                    shelf; deep water; lake; swamp.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="DISCOVERY" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Indicates if the well was the discovery well for the field containing
                                    it.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="PRODUCTS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="OBJECTIVE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of fluid that is the target objective during the
                                                well drilling operation
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PRIMARY" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="INJECTED" type="ihse_name_code" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="CONFIDENTIAL" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="FAULTED" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="FACTOR_N" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="SECONDARY_RECOVERY" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="PROJECT" type="ihse_name_code" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="INDICATORS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="IMAGES" type="xsd:boolean" minOccurs="0"/>
                                    <xsd:element name="LOGNET" type="ihse_name_code" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="LEGACY" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="DWIGHTS_STATUS" type="ihse_name_code" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="PLAY" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Information about unconventional play of a well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NAME" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:element name="LOCATION" type="ihse_location">
        <xsd:annotation>
            <xsd:documentation>Location information for the wellbore.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CORE">
        <xsd:annotation>
            <xsd:documentation>Information about a core sample taken in the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="METHOD" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="CODE" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="RUN_NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="ID_ADDITIONAL" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="SIDEWALL_RUN_NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="RECOVERY" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date of the coring operation.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="OPERATOR_DESIGNATION" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Core number or other designation assigned by the data vendor.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="SEQUENCE" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth of the cored interval.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the top of the cored interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the base of the cored interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="INTERVAL" type="ihse_length" minOccurs="0"/>
                                        <xsd:element name="RECOVERY" type="ihse_length" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DIAMETER" type="ihse_length" minOccurs="0"/>
                            <xsd:element name="SHAPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="ORIENTED" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Stratigraphic units present in the cored interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Stratigraphic unit at the top of the cored interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Stratigraphic unit at the base of the cored interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PRIMARY" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Primary stratigraphic unit in the cored interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <!-- <xsd:element name="METHOD" type="ihse_name_code" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>The type of procedure used to extract the core.
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element> -->
                            <xsd:element name="SHOW" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type of hydrocarbon show for the core.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="COUNT" type="xsd:long" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Number of cores taken in the interval.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="FLUID" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Type of drilling fluid used during coring.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="SHOTS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The total number of sidewall cores attempted during the
                                        operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="ATTEMPTED" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="RECOVERED" type="xsd:long" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="AVAILABLE" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="GAMMA" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="LAS" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="ANALYZED" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="SOLVENT" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="PERIODS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="EXTRACTION" type="ihse_period" minOccurs="0"/>
                                        <xsd:element name="DRYING" type="ihse_period" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="TEMPERATURE_DRYING" type="ihse_temperature" minOccurs="0"/>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional porosity
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="DESCRIPTION" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Description of the core sample.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth of core.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth to top of cored
                                                                interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth to base of cored
                                                                interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="INTERVAL" type="ihse_length" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Total length along which coring was
                                                                attempted.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="RECOVERY" type="ihse_length" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Length of the core recovered.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="LITHOLOGY" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Composition of the cored rock.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="POROSITY" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Porosity as determined by core analysis.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="INTERVAL" type="ihse_depth" minOccurs="0"/>
                                                    <xsd:element name="QUALITY" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Quality of the porosity in the core
                                                                sample.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Type of porosity in the core sample.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="SHOW" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Hydrocarbon show in the core sample.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The type of hydrocarbon show in the core
                                                                sample.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="INTERVAL" type="ihse_depth" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Remarks or comments about the core sample.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Rock unit present in core sample.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="ANALYSIS" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information pertaining to the laboratory analysis of porosity, permeability,
                            saturation, and density in a well core sample.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="DATE" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the well core sample was analyzed.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TYPES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Types of samples analyzed.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="PRIMARY" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The primary type of well core sample
                                                                analyzed.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="LABORATORY" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Name of laboratory supplying the analytical or
                                                    interpreted core analysis data.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="LABORATORY_LOCATION" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Location of laboratory supplying the analytical or
                                                    interpreted core analysis data.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ANALYST" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Name of individual performing well core sample
                                                    analysis or interpretation.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FILE_NUMBER" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>File number containing the core analysis data.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="LABORATORY_NUMBER" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Number or code assigned by the analysis laboratory to
                                                    the sample.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="SAMPLE" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Analysis data for a particular core sample.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="LABORATORY_NUMBER" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Number or code assigned by the analysis laboratory to
                                                    the core
                                                    sample.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth of core.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth to top of cored
                                                                interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth to base of cored
                                                                interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="INTERVAL" type="ihse_depth" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PERMEABILITIES" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="HORIZONTAL" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Horizontal permeability as determined by
                                                                core analysis.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="NINETY" type="ihse_permeability" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>Horizontal 90 degree
                                                                            permeability records the horizontal
                                                                            permeability measured 90 angular degrees
                                                                            from the Maximum horizontal permeability.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                                <xsd:element name="MAXIMUM" type="ihse_permeability" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>Maximum permeability measured
                                                                            in the well core sample.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                                <xsd:element name="PLUG" type="xsd:string" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The horizontal permeability
                                                                            measured from a plug.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                                <xsd:element name="RADIAL" type="xsd:string" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The horizontal permeability
                                                                            measured radially.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="VERTICAL" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Vertical permeability as determined by
                                                                well core sample analysis.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="AMOUNT" type="ihse_permeability" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>Vertical permeability in the
                                                                            well core sample.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="POROSITIES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Porosity as determined by well core sample
                                                    analysis.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="MAXIMUM" type="ihse_porosity" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Maximum porosity in the well core
                                                                sample.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="EFFECTIVE" type="ihse_porosity" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Porosity including only the
                                                                interconnected pores in the well core sample.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="SATURATIONS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="OIL_PORE" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="OIL_BULK" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="OIL_WEIGHT" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="GAS_VOLUME" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="WATER_PORE" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="WATER_BULK" type="ihse_decimal" minOccurs="0"/>
                                                    <xsd:element name="WATER_WEIGHT" type="ihse_decimal" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DENSITIES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured bulk density of the well core sample.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="GRAIN" type="ihse_density" minOccurs="0"/>
                                                    <xsd:element name="BULK" type="ihse_density" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="LITHOLOGY" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Composition of the cored rock.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MASSES" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="BULK" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OIL" minOccurs="0"/>
                                                                <xsd:element name="WATER" minOccurs="0"/>
                                                                <xsd:element name="SAND" minOccurs="0"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="GRAIN" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OIL" minOccurs="0"/>
                                                                <xsd:element name="WATER" minOccurs="0"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="METHOD" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="CODE" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="NUMBER" type="xsd:int" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="DENSITY" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="EXTRACT" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="SOLVENT" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="POROSITY" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="PERMEABILITY" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="FLUID_SATURATION" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="DRYING" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="DRYING_TIME" type="ihse_period" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="TEMP" type="ihse_period" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="EXTRACT_TIME" type="ihse_period" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                        <xsd:element name="CUTTING" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Narrative remarks about the core sample.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Narrative remark about the core sample.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
            <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LOG">
        <xsd:annotation>
            <xsd:documentation>Information about a geophysical log run.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="JOB_ID" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="RUN" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="SERVICE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The name of the most significant recorded service of the wireline
                                        survey.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depths from the surface datum to points in the logging
                                        run.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depth of the curve data at the top of the logging
                                                    pass.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depth of the curve data at the base of the logging
                                                    pass.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MAXIMUM_REACHED" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Maximum depth reached by the survey or test tool
                                                    during the log run.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BOTTOM_HOLE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Maximum depth of the borehole as recorded by the
                                                    driller during the log run.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TRACE_MINIMUM" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The minimum depth of the curve data recorded during
                                                    the logging pass.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TRACE_MAXIMUM" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The maximum depth of the curve data recorded during
                                                    the logging pass.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="TEMPERATURES" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Temperatures recorded during the logging run.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SURFACE" type="ihse_temperature" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Surface temperature during the logging run.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BOTTOM_HOLE" type="ihse_temperature" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Bottom hole temperature during the logging run.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MAXIMUM" type="ihse_temperature" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Maximum temperature recorded in the borehole during
                                                    the logging run.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Dates associated with the wireline log.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="LOGGING" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>TRIP_Date.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DIGITIZED" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the wireline log was digitized.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to geophysics.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional geophysics
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="MEDIA" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="STRING">
        <xsd:annotation>
            <xsd:documentation>Information on the tubing, casing or liners that are run into the well and the cement
                used.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Header information on the tubing, casing, or liners that are run into the
                            well.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE_LINER" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Type of liner.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="SIZES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="STRING" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Description of the outside diameter of the
                                                    tubular/pipe.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="HOLE" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measurement of the diameter of the tubular hole.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth of the string.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the top of the string.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the bottom of the string.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="REFERENCE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Reference from which depths are measured.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PACKER" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The depth the packer equipment was set to seal the
                                                    casing or tubing.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PENETRATION" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The footage amount that the casing has penetrated
                                                    beneath the sea floor surface.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MUD_ANCHOR" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The setting depth of a mud anchor in the wellbore.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="CEMENT_TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The depth to the top of the cement outside a
                                                    string.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="CEMENT_BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The depth to the base of the cement outside a
                                                    string.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="SEATING_NIPPLE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The depth of a seating nipple in a string.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="MIXED_STRING" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Y(es), N(o), or U(ndefined) indicating the casing is a mixed
                                        string.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="MATERIAL" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The material used for the well string.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="STRENGTH" type="ihse_decimal" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The axial load required to yield the pipe.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="COLLAR" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The type of collar used to couple the tubular with
                                                    another tubular.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="STEEL" type="ihse_decimal" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Manufacturer's specification under which the steel
                                                    was constructed.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DENSITY" type="ihse_density" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Density measurement (mass per unit volume) of the
                                                    tubular.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="WEIGHT" type="ihse_density" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The measured weight of the tubular (mass per unit
                                                    length).
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SET" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date string was set.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                        <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                        <xsd:element name="TUBING" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PRESSURE" type="ihse_pressure" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The highest pressure observed during the testing of a string.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="SCREEN" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Indicates if a screen was installed in the string.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="LENGTHS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PULLED" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Length of string removed from the wellbore during a
                                                    pulling or plugging operation.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="IN_HOLE" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Length of string left in the wellbore during a
                                                    pulling or plugging operation.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional string
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="CEMENT" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATE" type="ihse_date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Date the tubular observation occurred.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments about the well string.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional string
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
            <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CEMENT">
        <xsd:annotation>
            <xsd:documentation>Information about a cementing operation on the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The type of cement (and additive) used during a cementing operation.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="AMOUNT" type="ihse_volume" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The amount of cement used to secure the casing in place and prevent water and
                            other fluids
                            from entering the wellbore.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="GRADE" type="ihse_name_code" minOccurs="0"/>
                <xsd:element name="DEPTHS" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TOP" type="ihse_depth" minOccurs="0"/>
                            <xsd:element name="BASE" type="ihse_depth" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="SHOTS" type="ihse_decimal" minOccurs="0"/>
                <xsd:element name="RECEMENT" type="xsd:string" minOccurs="0"/>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments about the cement.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TREATMENT">
        <xsd:annotation>
            <xsd:documentation>Information describing stimulation treatment performed on the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="TEST_NUMBER" type="xsd:string" minOccurs="0"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code" minOccurs="0"/>
                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The type of treatment job.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0"/>
                <xsd:element name="DATES" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Various dates associated with the treatment operation.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="START" type="ihse_date" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="DEPTHS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Various depths associated with the treatment operation.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the top of the treated
                                        interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the base of the treated
                                        interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="FLUID" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The fluid injected into the well in the treatment operation.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="AMOUNT" type="ihse_fluid" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The amount of fluid used in the treatment operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type of fluid used in the treatment operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="PROPPING_AGENT" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The propant used in the hydraulic fracture treatment.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="AMOUNT" type="ihse_fluid" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of propping agent used in the treatment fluid.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Type of propping agent used in the treatment fluid.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="MESH_SIZE" type="ihse_value_string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Sieve mesh size for the given proppant agent.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="PRESSURES" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Various pressures associated with the treatment operation.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="BREAKDOWN" type="ihse_pressure" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The amount of pressure required to break the stratigraphic
                                        unit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="INSTANTANEOUS" type="ihse_pressure" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The instantaneous shutin pressure recorded after treatment fluid
                                        was injected into the stratigraphic unit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TREATMENT" type="ihse_pressure" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The average pressure under which treatment fluid was injected
                                        into the stratigraphic unit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="INJECTION_RATE" type="ihse_fluid" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The rate of fluid injection into the treated interval.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="SHUTIN_PRESSURE" type="ihse_pressure" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The instantaneous shutin pressure recorded after treatment fluid has been
                            injected into the formation
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ADDITIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>The type of additive used in the treatment fluid during the acidizing job.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="STAGE" type="xsd:long" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The stage of the fracturing treatment.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments about the treatment operation.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PERFORATION">
        <xsd:annotation>
            <xsd:documentation>Details about the perforation activity performed on a well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="COMPLETION" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Number of well completion which this perforation is
                                                    associated.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="STATUS" type="ihse_name_code" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The formations in the perforated interval</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The stratigraphic unit at the top of the perforated
                                                    interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The stratigraphic unit at the bottom of the
                                                    perforated interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depths of the perforated interval.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the top of the perforated
                                                    interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth to the base of the perforated
                                                    interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>

                            <xsd:element name="LENGTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Lengths of the perforated interval.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="LATERAL_GROSS_PERF" type="ihse_length" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="METHOD" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type of opening in the tubing for fluid to enter the
                                        tubing.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type of perforation method; for example, gun or jet.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PERFORATION" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="COUNT" type="xsd:long" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The total number of perforations or holes pierced through the
                                        casing or liner.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DENSITY" type="ihse_density" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Number of perforations used in combination with perforations per
                                        unit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments about the well perforation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="FAULT">
        <xsd:annotation>
            <xsd:documentation>Information about a faulted zone in the wellbore.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="IDENTIFICATION" type="xsd:string" minOccurs="0"/>
                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Code identifying the type of fault.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="DEPTHS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Depths of the faulted zone.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TOP_MEASURED" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the top of the fault zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TOP_TRUE_VERTICAL" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>True vertical depth from the surface to the top of the fault
                                        zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TOP_MISSING" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth to the top of the missing section measured in the
                                        correlation well.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE_MEASURED" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the bottom of the fault
                                        zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE_TRUE_VERTICAL" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>True vertical depth from the surface to the bottom of the fault
                                        zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE_MISSING" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth to the base of the missing section measured in the
                                        correlation well.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="THROW" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The vertical component of displacement on the fault.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The formations in the faulted zone.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The stratigraphic unit at the top of the faulted zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The stratigraphic unit at the base of the faulted zone.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="CORRELATION" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The ID number of the well used to correlate the fault.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="INTERPRETER" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The initials of the geologist picking the fault from the electrical log.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks pertaining to the fault.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LITHOLOGY">
        <xsd:annotation>
            <xsd:documentation>Information about physical characteristics of rocks in the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments about the lithology in the well.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional lithology data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="POROSITY">
        <xsd:annotation>
            <xsd:documentation>Details about the percent volume of pore space in the reservoir rock with a capacity for
                storing fluids.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="DEPTHS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Depths of the porous interval.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the top of the porous
                                        interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the base of the porous
                                        interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="THICKNESS_GROSS" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Value for the measured vertical thickness from the top of the
                                        highest permeable zone in the interval to the base of the lowest permeable zone
                                        in the interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="THICKNESS_QUALITY" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Code describing the quality of the sand thickness (e.g. +=minimum
                                        sand thickness, ?=questionable thickness, etc..
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="INTERVAL" type="ihse_depth" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The primary stratigraphic unit associated with the porous interval.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="PRIMARY" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Details about the primary stratigraphic unit associated with the
                                        porous interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="GROSS" type="ihse_porosity" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Value of the gross porosity for the porous interval.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="COAL_SEAMS" type="ihse_integer" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Number of coal seams associated with the horizon.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="INTERVAL_TYPE" type="ihse_value_string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The type of interval associated with the horizon. For example, COAL or
                            THICKNESS.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="QUALITY" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The quality of the porosity in the interval.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The primary type of porosity that is present in the porous interval.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="AGE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The stratigraphic unit age associated with the porous interval.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional porosity data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ABANDONMENT">
        <xsd:annotation>
            <xsd:documentation>Information on abandonment of the well, where operations have ceased and usually the well was plugged.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="DATES" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="ABANDONMENT" type="ihse_date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Date the well was abandoned.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                            <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Primary producing stratigraphic unit in the abandoned well.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="PRIMARY" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Primary producing stratigraphic unit in the abandoned well.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments about the abandonment of the well.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional abandonment data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="GEOLOGIST">
        <xsd:annotation>
            <xsd:documentation>Information on the geologist who performed the interpretation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments pertaining to the geologist.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="INTEREST">
        <xsd:annotation>
            <xsd:documentation>Information pertaining to the ownership of a specific well</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SOURCE" type="xsd:string"/>
                <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                <xsd:element name="PARTICIPANT" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>The participant in the well.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="PERCENT" type="xsd:double" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Percentage of interest owned by participant.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Type of interest the participant has under the terms of the lease
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Remarks or comments pertaining to well ownership.</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Reference to an external file that contains additional ownership data.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:group name="TEST_HEADER">
        <xsd:sequence>
            <xsd:element name="SOURCE" type="xsd:string"/>
            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
            <xsd:element name="NUMBER" type="xsd:string" minOccurs="0"/>
            <xsd:element name="RUN_NUMBER" type="xsd:long" minOccurs="0"/>
            <xsd:element name="REPORT_NUMBER" type="xsd:string" minOccurs="0"/>
            <xsd:element name="DATES" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TEST" type="ihse_date" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Date the production test was performed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0"/>
            <xsd:element name="TESTS" type="xsd:long" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Number of tests conducted on well.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="STATUS" type="ihse_name_code" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The final result of a well test. For example,
                        successful, misrun, pipe failure, packer failure, or tester plugged.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FORMATION_TEST_TYPE" type="ihse_name_code" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The type of stratigraphic unit test run on the well.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="COMPLETION" type="xsd:long" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>A sequential observation number used to uniquely identify a well completion.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="METHODS" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="COMPLETION" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type of aperture through which the fluid entered into the well
                                    tubing. For example, perforation, openhole or combination.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="PRODUCTION" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Method used to produce form well.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TEST" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The method of production (e.g., swabbing, flowing,
                                    pumping, and gas lift, etc.).
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="FLUIDS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Fluids recovered during test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="PREDOMINATE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The predominate type of fluid produced from the well during the
                                    test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="RECOVERED" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="AMOUNT" type="ihse_fluid" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="FLOWS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Details about the fluids that flowed during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="OIL" type="ihse_liquid" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the oil that flowed during the test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="CONDENSATE" type="ihse_liquid" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the condensate that flowed during the test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="GAS" type="ihse_gas" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the gas that flowed during the test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="CASINGHEAD_GAS" type="ihse_gas" minOccurs="0"/>
                        <xsd:element name="WATER" type="ihse_liquid" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the water that flowed during the test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="CALCULATED_ABSOLUTE_OPEN" type="ihse_fluid" minOccurs="0"/>
                        <xsd:element name="UNSPECIFIED" type="ihse_fluid" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="CUMULATIVE" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="GAS" type="ihse_gas" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="CHOKES" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The choke size during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TOP" type="ihse_choke" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The top choke diameter.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="BASE" type="ihse_choke" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The bottom choke diameter.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PERIODS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Duration of time required to conduct the production test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TOTAL" type="ihse_period" minOccurs="0"/>
                        <xsd:element name="PREFLOW" type="ihse_period" minOccurs="0"/>
                        <xsd:element name="INITIAL_SHUTIN" type="ihse_period" minOccurs="0"/>
                        <xsd:element name="FINAL_SHUTIN" type="ihse_period" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Information about the stratigraphic unit present in the tested interval.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the stratigraphic unit at the top of the tested
                                    interval.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the stratigraphic unit at the bottom of the tested
                                    interval.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="OBJECTIVE" type="ihse_stratigraphic_unit" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Details about the objective stratigraphic unit.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="DEPTHS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Depths of the production test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Measured depth from the surface to the top of the stratigraphic unit
                                    test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Measured depth from the surface to the base of the stratigraphic unit
                                    test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="INTERVAL_GROSS" type="ihse_depth" minOccurs="0"/>
                        <xsd:element name="INTERVAL_NET" type="ihse_depth" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="GRAVITIES" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Gravities of fluids produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="OIL" type="ihse_gravity" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Gravity of oil that flowed during the test.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="CONDENSATE" type="ihse_gravity" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Gravity of condensatel that flowed during the test.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="GAS" type="ihse_gravity" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Gravity of gas that flowed during the test.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="RATIOS" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Ratios of fluids recovered during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="GAS_OIL" type="ihse_ratio" minOccurs="0"/>
                        <xsd:element name="CONDENSATE" type="ihse_ratio" minOccurs="0"/>
                        <xsd:element name="WATER" type="ihse_ratio" minOccurs="0"/>
                        <xsd:element name="BASIC_SEDIMENT_WATER" type="ihse_ratio" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="FACTORS" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Z" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="OVER_Z" type="ihse_ratio" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="GAS_CONTENT" type="ihse_name_code" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The gas content, other than hydrocarbons, that are present during the test (e.g.,
                        H2S, CO2, and N2, etc.).
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SULPHUR" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="INDICATOR" type="ihse_name_code" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="HOLE" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ENVIRONMENT" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="SIZE" type="ihse_length" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PRESSURES" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Information about pressures measured during various periods of the test.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="SHUTIN" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Information about shutin pressures.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="INITIAL" type="ihse_pressure" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The initial shutin pressure.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FINAL" type="ihse_pressure" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The final shutin pressure.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="FLOWING" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Information about flowing pressures.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="INITIAL" type="ihse_pressure" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The initial shutin pressure.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FINAL" type="ihse_pressure" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The final shutin pressure.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="CASING" type="ihse_pressure" minOccurs="0"/>
                        <xsd:element name="BOTTOM_HOLE" type="ihse_pressure" minOccurs="0"/>
                        <xsd:element name="UNSPECIFIED" type="ihse_pressure" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="SHOW" type="ihse_name_code" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The hydrocarbon show associated with the recovery fluid. For example, oil, gas,
                        or both.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The temperature recorded during a well test.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RECORDER" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DEPTH" type="ihse_depth" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="PERFORMANCE" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="MODEL" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Remarks or comments about the production test.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Reference to an external file that contains additional production test data.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:group name="IP_PT_BASE">
        <xsd:annotation>
            <xsd:documentation>Information about initial potential tests/production tests.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="HEADER">
                <xsd:complexType>
                    <xsd:group ref="TEST_HEADER"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="PERFORATION" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="TREATMENT" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="PRESSURE" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about pressures during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="HEADER">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the top of
                                                            the tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the base
                                                            of the texted interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DATUM_POOL" type="ihse_depth" minOccurs="0"/>
                                                <xsd:element name="DATUM_WELL" type="ihse_depth" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Pressures during the production test.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="FLOWING" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The tubing pressure measured
                                                                        under flowing conditions.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The casing pressure measured
                                                                        under flowing conditions (the pressure built up
                                                                        between the casing and the tubing).
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="SHUTIN" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The shutin pressure measured in
                                                                        the tubing
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The casing pressure measured
                                                                        under shutin conditions.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Rock units in the tested interval.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Stratigraphic unit present at the top of the
                                                            tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Stratigraphic unit present at the base of the
                                                            tested interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="BOTTOM_HOLE" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Bottom hole conditions during the test.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                    <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depths associated with the bottomhole pressure test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="PACKER" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The depth the packer equipment was set to
                                                            seal the casing or tubing.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="RUN" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The depth at which the bottom hole pressure
                                                            reading was taken by the recorder.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="SEA_LEVEL_DATUM" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The sea level reference datum. This is
                                                            calculated by taking elevation and subtracting the recorder
                                                            depth. The value has a (-) for below sea level.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole pressure during the test.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="FLOWING" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="WELLHEAD" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="DATUM" type="ihse_pressure" minOccurs="0"/>
                                                <xsd:element name="GRADIENT" type="ihse_pressure_gradient" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Time duration required to conduct the bottom hole
                                                pressure test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Bottom hole temperature during the bottom hole pressure
                                                test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TUBING_SIZE" type="ihse_length" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Description of the tubing size used during the bottom
                                                hole pressure test
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ABSOLUTE_OPEN_FLOW" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Absolute open flow well pressure information pertaining to the
                                    production of oil or gas under wide-open conditions (the flow of production from a
                                    well without any restrictions on the rate of flow).
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="HEADER">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                                                <xsd:element name="FLOWS" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Open flows.</xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="CALCULATED" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Calculated open flow potential.
                                                                        The theoretical rate that a well would flow at
                                                                        the wall of the well with zero pounds pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FOUR_POINT" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The calculated absolute open flow
                                                                        based on a 4 point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="PRESSURES" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Pressures associated with the absolute open
                                                            flow pressure.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="SHUTIN" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The instantaneous shut-in
                                                                        pressure measured in a four point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The average reservoir pressure.
                                                                        This is the shut-in pressure for a four point
                                                                        test and referred to as formation/reservoir
                                                                        pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="INVERSE" type="xsd:double" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The slope of the stabilized deliverability
                                                            line. The points of the 4pt test are plotted and the slope
                                                            determines optimum flow rate.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="FOUR_POINT" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about a four point test conducted in the
                                                well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                <xsd:element name="CHOKE" type="ihse_size" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Choke size during the four point test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FLOW" type="ihse_fluid" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The stablized flow rate for the test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="PRESSURES" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Pressures during the four point test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="FLOW" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The sandface pressure at the
                                                                        stabilized flow rate.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="BOTTOM_HOLE" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The bottom hole pressure during
                                                                        the four point test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The duration of the absolute open flow
                                                            period.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_WATER" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of water produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type of fluid being analyzed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit present at the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit present at the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TEMPERATURE"  minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RESERVOIR"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="UOM_ORIGINAL" type="xsd:string"  minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="RW"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:double"  minOccurs="0"></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string"  minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                        <xsd:element name="RESISTIVITY" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="VALUE" type="xsd:double" minOccurs="0">
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                        <xsd:element name="SALINITY" type="ihse_salinity_water_analysis" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The total weight of dissolved salts in the water sample.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TYPE_NAME" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="VALUE" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="DESCRIPTION" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="UOM" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        <xsd:element name="LABORATORY_REFERENCE" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_OIL" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of oil produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="RATIOS" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="GAS_OIL" type="ihse_ratio" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units present in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the top of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the bottom of the tested
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="VISCOSITY" minOccurs="0" >
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:int" minOccurs="0" ></xsd:element>
                                    <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                    <xsd:element name="UOM_ORIGINAL" type="xsd:string" minOccurs="0" ></xsd:element>
                                    <xsd:element name="UOM" type="xsd:string"  minOccurs="0"  ></xsd:element>
                                    <xsd:element name="TYPE" type="xsd:string"  minOccurs="0"  ></xsd:element>
                                    <xsd:element name="POUR_POINT" minOccurs="0" >
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string" minOccurs="0" ></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TEMPERATURE" minOccurs="0" >
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int" minOccurs="0" ></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string" minOccurs="0" ></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ANALYSIS_GAS" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about analysis of gas produced during the test.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Rock units present in the tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the top of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Stratigraphic unit at the base of the tested interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:choice minOccurs="0" maxOccurs="unbounded">
                            <xsd:element name="CARBON_DIOXIDE" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of carbon dioxide in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HEAT_CONTENT" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Heat content of the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="GRAVITY" type="ihse_gravity" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Gravity of the gas produced during the test.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HELIUM" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of helium in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="NITROGEN" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of nitrogen in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="HYDROGEN_SULFIDE" type="ihse_decimal" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Amount of hydrogen sulfide in the gas produced during the test.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="FLUID" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TYPE" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of tested interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0"/>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="FIELD_DATA" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about the field where the well is located.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="DISCOVERY_WELL" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Y(es) or N(o) indicating the well is a discovery well. A discovery is
                                    an exploratory well that has penetrated a new oil/gas producing reservoir or field.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="FIELD" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Name of the field where the well is located.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SUB_HORIZON" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Number identifying each producing interval in a well in chronological
                                    order (e.g., 1 = original producing interval, 2 = deepened producing
                                    interval, etc.).
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="OPERATORS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Organizations that operated the well.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="CURRENT" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The current operator of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ORIGINAL" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The original operator of the well.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="TOP_PAY" type="ihse_depth" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Measured depth from the surface to the top of the pay zone.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="LEASE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Name assigned to the lease by the regulatory agency with jurisdiction
                                    over the minerals in the area where the lease is located.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="SHUTOFF" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about the shutoff used in the wellbore.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>The type of shutoff used in the wellbore (e.g., bridge
                                    plug, cased off, plugged off, or squeezed, etc.).
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="DEPTHS" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Depths of the shutoff interval.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the top of the shutoff
                                                interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the base of the
                                                shutoff interval.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PLUGBACK" type="ihse_distance" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Measured depth from the surface to the cement plug.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Narrative remarks about the initial potential/production tests.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>Narrative remarks about the initial potential/production tests.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:element name="GEOLOGY">
        <xsd:annotation>
            <xsd:documentation>Information on the geology in the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="CORE" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="GEOLOGIST" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="LITHOLOGY" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="POROSITY" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="PAYZONE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about the pay zone.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="FLUID" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="ID" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depths of the pay zone</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the top of the pay
                                                    zone.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the base of the
                                                    pay zone.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OIL_WATER" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="GAS_OIL" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="GAS_WATER" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="PAY_GROSS" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="PAY_NET" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="HYDROCARBON" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional pay zone
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="FAULT" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="SHALE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a shale zone in the well.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Code identifying the shale event type. The type identifies the
                                        shale resistivity or depth being measured. For example, the depth picked off the
                                        electric log where the resistivity has decreased to 1/2 Ohm.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DEPTH" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The depth of the shale event that was measured. For example, the
                                        depth of the 1/2 Ohm shift.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="RESISTIVITY" type="ihse_resistivity" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The resistivity measured in the shale. The resistivity of shale
                                        is its ability to impede the flow of electrical current.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional shale
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SURVEYS">
        <xsd:annotation>
            <xsd:documentation>Surveys to determine the physical geometry of the wellbore.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="BOREHOLE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>The hole made by the bit during the drilling operation.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SOURCE" type="xsd:string"/>
                                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="RUN_NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="QUALITY" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="COMPANIES" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="FINAL" type="ihse_name_code" minOccurs="0"/>
                                                    <xsd:element name="ORIGINAL" type="ihse_name_code" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="METHODS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="SURVEY" type="ihse_name_code" minOccurs="0"/>
                                                    <xsd:element name="PROCESSING" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The type of survey processing.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="CALCULATION" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The method used to calculate the survey
                                                                data.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REPORT_TYPE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Identifies the type of directional survey report that
                                                    was used for the selected depth interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DATES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Dates the survey was performed.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="START" type="ihse_date" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Date the survey started.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="END" type="ihse_date" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Date the survey ended.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depths of the survey.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                top of surveyed interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                bottom of surveyed interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="RESAMPLE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>A filtering or re-sampling interval
                                                                applied. This re-sampling normally only applies to
                                                                dipmeter derived
                                                                directional surveys that may have an unnecessary spacing
                                                                between surveys points.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="NORTH_REFERENCE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Valid north references used in surveying to define
                                                    angular measurements.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MAP_PROJECTION" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The type of map projection used for the borehole
                                                    location data.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ZONE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>indicates the state plane coordinate system zone or
                                                    the UTM zone where the well is located. UTM zones number from 1 to
                                                    60.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="NORTH_CORRECTION" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Value in degrees of the declination angle at the
                                                    survey location.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="DIRECTION" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Designates the direction from true North
                                                                to
                                                                reference North.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="ANGLE" type="ihse_decimal" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Value in degrees of the angle measured
                                                                between true north and the referenced north.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="REFERENCE" type="ihse_name_code" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The correction angle type.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="GEOGRAPHIC" type="ihse_lat_long" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="MAGNETIC_DECLINATION" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="DIRECTION" type="ihse_name_code" minOccurs="0"/>
                                                    <xsd:element name="ANGLE" type="ihse_decimal" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="GRID_CONVERGENCE" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="DIRECTION" type="ihse_name_code" minOccurs="0"/>
                                                    <xsd:element name="ANGLE" type="ihse_decimal" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PLANE_PROPOSAL" type="ihse_offset" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The value in degrees of the proposed horizontal angle
                                                    or azimuth of the total vertical section. The total vertical section
                                                    is the measure of how far the borehole has moved in the same
                                                    direction as a straight line from the surface location to the
                                                    proposed target. The vertical sections are calculated by using the
                                                    right triangle formed
                                                    between the borehole path and the straight line. The plane of
                                                    proposal is the azimuth of the vertical section.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="SHOT" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="EXTRAPOLATION" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="SAMPLES" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="TIE_POINT" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Location of the initial point of the survey.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="NORTH_SOUTH" type="ihse_offset" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Coordinate of tie point in the North or
                                                                South direction.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="EAST_WEST" type="ihse_offset" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Coordinate of tie point in the East or
                                                                West direction.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="DEPTHS" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>True vertical depth of tie
                                                                            point.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="UNCERTAINTY" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Information about the uncertainty in each point
                                                    measurement.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="DISTANCE" type="ihse_distance" minOccurs="0"/>
                                                    <xsd:element name="REASON" type="ihse_name_code" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REMARK_PROCESSING" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Processing comments from bore path calculation
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Remarks or comments about the survey.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:annotation>
                                                <xsd:documentation>Reference to an external file that contains
                                                    additional survey data
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="POINT" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Details about a directional survey point.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The type of point.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depths of the point survey.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="MEASURED" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                top of the surveyed interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                bottom of the surveyed interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DEVIATION" type="ihse_angle" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Value for the angle of deviation or inclination away
                                                    from the vertical axis of the wellbore at the
                                                    survey point.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="NORTH_SOUTH_COORDINATE" type="ihse_offset" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The positional offset, in the north/south (Y)
                                                    direction, from the well surface hole to the directional survey
                                                    point.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="EAST_WEST_COORDINATE" type="ihse_offset" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The positional offset, in the east/west (X)
                                                    direction, from the well surface hole to the directional survey
                                                    point.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TARGET_OFFSET" type="ihse_distance" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>A measure of how far the borehole has moved in the
                                                    same direction as a straight line from the surface location to the
                                                    proposed target.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DOGLEG" type="xsd:double" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Value for the dogleg severity, the rate of change of
                                                    deviation angle and/or direction evaluated between two survey
                                                    points.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DELTA" type="ihse_lat_long" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Change in Latitude and Longitude from previous
                                                    point.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PROJECTED_VALUE" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Values for point projected.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="OVERLAPPED" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Values for point overlap data from other surveys.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="END_POINT" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>This point is the end point of a survey.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Stratigraphic unit within which the survey point exists.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
                        <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DRILLING">
        <xsd:annotation>
            <xsd:documentation>Detailed drilling information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="BIT" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a drill bit used during the drilling operation.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The name of the service company supplying the drill bit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type or make of the drill bit used.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="SIZE" type="ihse_size" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The diameter (width) of the drilling bit.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The depths at which the bit was used.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="RUN_IN" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The measured depth of the well when the bit started
                                                    drilling.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="RUN_OUT" type="ihse_distance" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The measured depth of the well when the bit was
                                                    pulled.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="INTERVAL" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured length of the section the bit drilled.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="WEIGHTS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The amount of force applied to the drill bit during the drilling
                                        operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="MINIMUM" type="ihse_weight" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The least amount of force applied to the bit during
                                                    the drilling operation
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="MAXIMUM" type="ihse_weight" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The greatest amount of force applied to the bit
                                                    during the drilling operation
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="RATE" type="ihse_rate" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Value for the average speed of rotation of the bit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Length of time bit was used.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the drill bit.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional drill bit
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="FISH" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a fishing operation to retrieve an item from the
                            wellbore.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="FISH" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the fishing operation occurred.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the fishing operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional
                                        information about the fishing operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="BREAK" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a change in drilling rate during drilling operations.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="BREAK" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the drilling break occurred.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth at which the drilling break occurred.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the top of the
                                                    drilling break.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the base of the
                                                    drilling break.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Stratigraphic unit within which the drilling break occurred.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="SHOW" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Hydrocarbon or water show at break.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the drilling break.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional drilling
                                        break data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="PROBLEM" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about problems encountered during the drilling operation.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PROBLEM" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date problem occurred.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the problem.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional
                                        information about the problem.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="LOST_CIRCULATION" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a loss of circulation during drilling operations.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Dates of lost circulation.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="LOSS" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the loss of circulation occurred.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="CONTROL" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the lost circulation was brought under
                                                    control.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="MUD_LOST" type="ihse_volume" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The volume of drilling fluid lost in the lost circulation
                                        interval.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth of loss of circulation.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the top of the
                                                    interval where lost circulation occurred.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the base of the
                                                    interval where lost circulation occurred.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="SEVERITY" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The severity of the lost circulation event.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Stratigraphic unit within which the lost circulation occurred.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the loss of circulation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional loss of
                                        circulation data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="MEDIA" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="PLUGBACK" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about a plugback operation in the well.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PLUGBACK" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date of the plugback operation.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depth of the plugback interval.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the top of the
                                                    plugback interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the base of the
                                                    plugback interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="CEMENT" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Information about cement used on the plugback operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="AMOUNT" type="ihse_volume" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Quantity of cement used to secure the plug and
                                                    isolate the interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="GRADE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Grade of cement used to secure the plug and isolate
                                                    the interval.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                        <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments about the plugback operation.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional plugback
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="SHOW" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about hydrocarbon shows in the well.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SOURCE" type="xsd:string"/>
                                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The depth of the hydrocarbon show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                top of the show interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the
                                                                bottom of the show interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Description of the lithology associated with the
                                                    hydrocarbon show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The stratigraphic unit at the top of the
                                                                show interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The stratigraphic unit at the bottom of
                                                                the show interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="SAMPLE_TYPE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Type of sample used to determine the type of show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="LITHOLOGY" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Description of the lithology associated with the
                                                    hydrocarbon show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Remarks or comments pertaining to the show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:annotation>
                                                <xsd:documentation>Reference to an external file that contains
                                                    additional show data.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Narrative remarks pertaining to the show.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Narrative remarks pertaining to the show.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TYPE" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="WHIPSTOCK" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Details about a whipstock, used for sidetracking or directional drilling.
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="WHIPSTOCK" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the whipstock was put into the well.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FIRST_REPORT" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="LAST_UPDATE" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTH" type="ihse_depth" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Measured depth from the surface to the whipstock.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remarks or comments pertaining to the whipstock.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional whipstock
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="BLOWOUT" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PRIMARY" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="HORIZONTAL" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Narrative remarks pertaining to the drilling.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DATE" type="ihse_date" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Date of narrative remarks.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Narrative remarks pertaining to the drilling</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional drilling
                                        data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="GEOPHYSICS">
        <xsd:annotation>
            <xsd:documentation>Information about geophysical logging in the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="LOG" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MECHANICAL">
        <xsd:annotation>
            <xsd:documentation>Information about the well string.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="STRING" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PRIOR_INFORMATION">
        <xsd:annotation>
            <xsd:documentation>Prior information about the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="WELL_NUMBER" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Prior well number information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="PRIOR" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The prior well number associated with the well.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional prior well
                                        number data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="LEASE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Prior lease information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="PRIOR" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The prior lease name associated with the well.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional prior
                                        lease data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="OPERATOR" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Prior operator information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="PRIOR" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The prior operator associated with the well.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional prior
                                        operator data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="IDENTIFICATION" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="PRIOR" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:group name="WELL_TESTS">
        <xsd:annotation>
            <xsd:documentation>Information about the test used to evaluate the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="IP_PT" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about initial potential/production tests.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:group ref="IP_PT_BASE"/>
                    </xsd:sequence>
                    <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
                    <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="DRILL_STEM" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Information about drill stem tests.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="HEADER">
                            <xsd:complexType>
                                <xsd:group ref="TEST_HEADER"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Information about the stratigraphic unit present in the tested
                                    interval.
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="PIPE_RECOVERY" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Information about the material recovered from the pipe during the
                                    test.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Code representing the type of material recovered during
                                                the formation test (e.g., oil, gas, water, or condensate, etc.).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="METHOD" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>How the fluid was obtained (e.g., pipe chamber).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="AMOUNT" type="ihse_fluid" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>the amount of material recovered.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DESCRIPTION" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>A description of the fluid recovered.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="REVERSE_CIRCULATION" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Indicates if reverse circulation was used.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="SHOW" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Show of petroleum in recovered fluid.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>

                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                        <!-- -->
                        <xsd:element name="RECORDER" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:string" minOccurs="0"></xsd:element>
                                    <xsd:element name="PRESSURE" minOccurs="0"></xsd:element>
                                    <xsd:element name="USED" type="xsd:string" minOccurs="0"></xsd:element>
                                    <xsd:element name="DEPTH" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int" minOccurs="0"></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string" minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="TYPE" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TYPE" type="xsd:string" minOccurs="0"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PERFORMANCE" type="xsd:string" minOccurs="0"></xsd:element>
                                    <xsd:element name="TEMPERATURE" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="MAXIMUM" type="ihse_temperature" minOccurs="0">
                                                </xsd:element>
                                                <xsd:element name="MINIMUM" type="ihse_temperature" minOccurs="0">
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="POSITION" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="CODE" type="xsd:string"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>

                                    <xsd:element name="RELATIVE_TO_TOOL" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="CODE" type="xsd:string"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="EQUIPMENT" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:int" ></xsd:element>
                                    <xsd:element name="TYPE" type="xsd:string"></xsd:element>
                                    <xsd:element name="LENGTH" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int"></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="DEPTH"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="VALUE" type="xsd:int"></xsd:element>
                                                            <xsd:element name="UOM" type="xsd:string"></xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="WEIGHT"  minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="VALUE" type="xsd:int"></xsd:element>
                                                <xsd:element name="UOM" type="xsd:string"></xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="DIAMETER" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="INSIDE" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="VALUE" type="xsd:int"></xsd:element>
                                                            <xsd:element name="UOM" type="xsd:string"></xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="OUTSIDE" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="VALUE" type="xsd:int"></xsd:element>
                                                            <xsd:element name="UOM" type="xsd:string"></xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="REMARK"  minOccurs="0"></xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <!-- -->

                        <xsd:element name="PERIOD" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="HEADER">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" minOccurs="0"/>
                                                <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                                <xsd:element name="PERIODS" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Time durations of test periods.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="SHUTIN" type="ihse_period" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The shut-in period. The duration  of time that the well was shut-in allowing the
                                                                        pressure to build up to the stratigraphic unit pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FLOWING" type="ihse_period" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The duration of time that the well was flowing.                                                                   </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="HYDROSTATIC" type="ihse_period" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The duration of hydrostatic pressure well test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="CHOKE" type="ihse_size" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The choke size during the test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about pressures measured during various
                                                periods of the test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="SHUTIN" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Information about shutin pressures.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="INITIAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The initial shutin pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FINAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The final shutin pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Remarks or comments pertaining to
                                                                        the shutin pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="FLOWING" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Information about flowing pressures.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="INITIAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The initial flowing pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FINAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The final flowing pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Remarks or comments pertaining to
                                                                        the flowing pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="HYDROSTATIC" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Information about hydrostatic pressures.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="INITIAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The initial hydrostatic
                                                                        pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="FINAL" type="ihse_pressure" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The final hydrostatic pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Remarks or comments pertaining to
                                                                        the hydrostatic pressure.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="MATERIALS_TO_SURFACE" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about materials that flowed to the surface.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="HEADER">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                            <xsd:element name="FLUID" type="ihse_name_code" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Information about fluid that
                                                                        flowed to the surface during the test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="RATE" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The amount of fluid that flowed
                                                                        to the surface during the test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="PERIODS" minOccurs="0">
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="TO_SURFACE" type="ihse_period" minOccurs="0">
                                                                            <xsd:annotation>
                                                                                <xsd:documentation>Time it took material
                                                                                    to reach the surface.
                                                                                </xsd:documentation>
                                                                            </xsd:annotation>
                                                                        </xsd:element>
                                                                    </xsd:sequence>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="FLOW" minOccurs="0" maxOccurs="unbounded">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Information about the flow of fluids to the
                                                            surface.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                            <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The flowing stage or period that
                                                                        the material flowed to surface. For example, the
                                                                        material flowed to surface during the initial
                                                                        flow, final flow or maximum flow period.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="AMOUNT" type="ihse_fluid" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Volume of fluid flowing to the
                                                                        surface during a drill stem test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Remarks or comments pertaining to
                                                                        fluid flow to the surface.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="ANALYSIS" minOccurs="0" maxOccurs="unbounded">
                                        <xsd:annotation>
                                            <xsd:documentation>Information about analysis of fluids recovered during the
                                                drill stem test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                <xsd:element name="FLUID" type="ihse_name_code" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Type of fluids recovered during the test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="RATIOS" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Gas/oil ratio of fluid recovered during the
                                                            test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="GAS_OIL" type="ihse_ratio" minOccurs="0"/>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="GRAVITIES" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Gravities of fluids recovered during the
                                                            test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="OIL" type="ihse_gravity" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Gravity of oil recovered during
                                                                        the test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="SALINITY" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Salinity of fluids recovered during the
                                                            test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="REFERENCE" type="ihse_name_code" minOccurs="0"/>
                                                            <xsd:element name="AMOUNT" type="ihse_salinity" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>The amount of salinity.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="WATER" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Analysis of water recovered during the
                                                            test.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="RESISTIVITY" type="ihse_resistivity" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Resistivity of water recovered
                                                                        during the test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                            <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                                                                <xsd:annotation>
                                                                    <xsd:documentation>Temperature of the water
                                                                        recovered during the test.
                                                                    </xsd:documentation>
                                                                </xsd:annotation>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="PERMEABILITY" type="ihse_permeability" minOccurs="0"/>
                                                <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="SHUTOFF" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Information describing the method used to shut off open hole.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of shutoff used in the wellbore.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Depths of the shutoff interval.</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the top of
                                                            the shutoff interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the base
                                                            of the shutoff interval.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="PLUGBACK" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>Measured depth from the surface to the cement
                                                            plug.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element ref="MEDIA" minOccurs="0"/>
                        <xsd:element name="ANALYSIS_COMPUTED" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Computed analysis from testing companies.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="COMPANY" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>the company or laboratory which performed the
                                                well computed analysis.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PERMEABILITY" type="ihse_permeability" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Value for the measured permeability of the formation (the
                                                measure of the ease with which a fluid flows through a rock).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DAMAGE" type="xsd:double" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Estimated damage ratio. Undamaged permeability of the
                                                reservoir divided by the permeability measured during a DST. Reflects
                                                the skin damage.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PRESSURES" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The extrapolated maximum reservoir pressure
                                                            (initial shut-in pressure) computed from the plotted gauge
                                                            information.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="RADIUS_OF_INVESTIGATION" type="ihse_distance" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The radius of investigation (the distance from the hole
                                                that was investigated by the testing apparatus).
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DEPTHS" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="GAUGE" type="ihse_depth" minOccurs="0">
                                                    <xsd:annotation>
                                                        <xsd:documentation>The measured depth of the pressure recording
                                                            device a the time of the analysis.
                                                        </xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Any additional narrative comments about the computed
                                                analysis of the well.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="CUSHION" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Information about the cushion used during a drill stem test to buffer
                                    the stratigraphic unit being tested from the effects of radical pressure change.
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The type of cushion used.</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="INTERVAL" type="ihse_length" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>The depth interval where the cushion was used.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>

                                    <xsd:element name="PRESSURE" type="ihse_pressure" minOccurs="0"/>
                                    <xsd:element name="INHIBITOR" type="ihse_decimal" minOccurs="0"/>
                                    <xsd:element name="FLUID" type="ihse_decimal" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>Narrative remarks about the drill stem test.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation>Remarks or comments about the drill stem test.
                                            </xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
                    <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="UNSPECIFIED" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Test type is not known.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="HEADER">
                            <xsd:complexType>
                                <xsd:group ref="TEST_HEADER"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                    <xsd:element name="REMARK" type="xsd:string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="TYPE_NAME" type="xsd:string" use="optional"/>
                    <xsd:attribute name="TYPE_CODE" type="xsd:string" use="optional"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>
    <xsd:element name="OWNERSHIP">
        <xsd:annotation>
            <xsd:documentation>Information about the ownership of the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="INTEREST" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OPERATION">
        <xsd:annotation>
            <xsd:documentation>Information pertaining to abandonment of the well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="ABANDONMENT" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MEDIA">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The type of drilling media.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="OPERATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Depths where the drilling media was used.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the point where
                                                    the operator started using or changed the drilling media.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth from the surface to the point where
                                                    the operator ceased using or changed the drilling media.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="WEIGHT" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Measured depth of the wellbore when the drilling
                                                    media sample was taken.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="CASING" type="ihse_depth" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depth of casing in well.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DENSITIES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="INITIAL" type="ihse_density" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The density of the drilling media before a drilling
                                                    event.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FINAL" type="ihse_density" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The density of the drilling media after a drilling
                                                    event.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="VISCOSITIES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="INITIAL" type="ihse_viscosity" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The viscosity of the drilling media before a drilling
                                                    event.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FINAL" type="ihse_viscosity" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The density of the drilling media after a drilling
                                                    event.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PH" type="xsd:double" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The pH of the drilling media.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="FLUID_LOSS" type="ihse_fluid" minOccurs="0"/>
                            <xsd:element name="SINCE_CIRCULATION" type="ihse_period" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Elapsed time since the circulation process stopped.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="WEIGHT" type="ihse_weight" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Weight of the drilling media.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="RESISTIVITY" type="ihse_resistivity" minOccurs="0"/>
                            <xsd:element name="FILTRATE" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="RESISTIVITY" type="ihse_resistivity" minOccurs="0"/>
                                        <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0"/>
                                        <xsd:element name="SALINITY" type="ihse_salinity" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PRESSURE" type="ihse_pressure" minOccurs="0"/>
                            <xsd:element name="SALINITY" type="ihse_salinity" minOccurs="0"/>
                            <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0"/>
                            <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Remark text about media.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional drilling
                                        media data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="RESISTENCE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Information about the resistivity of the drilling media.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Type of sample being measured (e.g., mud, mud cake or
                                        filtrate).
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="RESISTIVITY" type="ihse_resistivity" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>A surface measurement of the drilling fluid resistivity at a
                                        reported temperature.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Described temperature for measuring resistivity.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ATTACHMENT" type="ihse_attachment" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Reference to an external file that contains additional media
                                        resistivity data.
                                    </xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="STAGE">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="DESIGNATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="STATUSES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="INITIAL" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="FINAL" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="TECHNICAL" type="ihse_name_code" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="CLASSES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="INITIAL" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="FINAL" type="ihse_name_code" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="PRODUCTS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="PRIMARY" type="ihse_name_code" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DATES" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="START" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="END" type="ihse_date" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="OPERATOR" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="GROUP" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DEVIATION" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="RIG" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="IDENTIFICATION" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="CONTRACTOR" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="PERIOD" type="ihse_decimal" minOccurs="0"/>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TOTAL_MEASURED" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="TOTAL_TRUE_VERTICAL" type="ihse_depth" minOccurs="0"/>
                                        <xsd:element name="FOOTAGE" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="HISTORY" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="ACTIVITY" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="EVENT" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="DATE" type="ihse_date" minOccurs="0"/>
                                        <xsd:element name="DEPTH_MEASURED" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="REMARK" type="xsd:string"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="NARRATIVE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                            <xsd:element name="REMARK" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ENGINEERING">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="COMPLETION" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Details about the completions performed on a well.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SOURCE" type="xsd:string"/>
                                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="ACTIVITY" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0"/>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="DATES" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="COMPLETION" type="ihse_date" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="METHOD"  minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="CODE" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="NAME" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element ref="PERFORATION" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element ref="PERFORATION" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="PRESSURE" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                        <xsd:documentation>Details about the pressure tests performed on a well.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Summary information about pressure tests.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SOURCE" type="xsd:string"/>
                                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                                        <xsd:element name="TEST_TYPE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The type of pressure test conducted on the
                                                    wellbore.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>A sequence number uniquely identifying the production test being performed in the well.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="FLUID_TYPE" type="xsd:string" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Fluid type of GAS or OIL</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depths of the tested interval.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the top of the tested interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Measured depth from the surface to the base of the texted interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PRESSURES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Pressures during the production test.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="FLOWING" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The tubing pressure measured
                                                                            under flowing conditions.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                                <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The casing pressure measured
                                                                            under flowing conditions (the pressure built up
                                                                            between the casing and the tubing).
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="SHUTIN" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="TUBING" type="ihse_pressure" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The shutin pressure measured in
                                                                            the tubing
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                                <xsd:element name="CASING" type="ihse_pressure" minOccurs="0">
                                                                    <xsd:annotation>
                                                                        <xsd:documentation>The casing pressure measured
                                                                            under shutin conditions.
                                                                        </xsd:documentation>
                                                                    </xsd:annotation>
                                                                </xsd:element>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="RESERVOIR" type="ihse_pressure" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="STRATIGRAPHIC_UNITS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Rock units in the tested interval.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="TOP" type="ihse_stratigraphic_unit" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Stratigraphic unit present at the top of the
                                                                tested interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="BASE" type="ihse_stratigraphic_unit" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>Stratigraphic unit present at the base of the
                                                                tested interval.
                                                            </xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="BOTTOM_HOLE" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Bottom hole conditions during the test.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="SOURCE" type="xsd:string"/>
                                        <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Number identifying the bottom hole pressure observation.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="PRESSURE_NUMBER" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Sequence number uniquely identifying the production test being
                                                    performed in the well.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>The type of pressure test conducted on the wellbore.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DATE" type="ihse_date" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Date the bottom hole pressure test was performed.</xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Depths associated with the bottomhole pressure test.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="RUN" type="ihse_depth" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The depth at which the bottom hole pressure reading was taken by the recorder.</xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PRESSURES" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Bottom hole pressure during the test.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="FLOWING" type="ihse_pressure" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The measured pressure taken at the recorder depth. This is the static bottom hole pressure measured after the well has been shut in.</xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="WELLHEAD" type="ihse_pressure" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The pressure on the fluid at the top of the well after the well has been shut in for a period of time.</xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="DATUM" type="ihse_pressure" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The bottom hole pressure corrected to sea level.</xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                    <xsd:element name="GRADIENT" type="ihse_pressure" minOccurs="0">
                                                        <xsd:annotation>
                                                            <xsd:documentation>The pressure gradient near the run depth.</xsd:documentation>
                                                        </xsd:annotation>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="PERIOD" type="ihse_period" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Time duration required to conduct the bottom hole pressure test.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TEMPERATURE" type="ihse_temperature" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Bottom hole temperature during the bottom hole pressure test.
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="TUBING_SIZE" type="ihse_length" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Description of the tubing size used during the bottom hole pressure test
                                                </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HORIZONTAL">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="HEADER">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="SOURCE" type="xsd:string"/>
                            <xsd:element name="DATA_PROVIDER" type="ihse_name_code"/>
                            <xsd:element name="IDENTIFICATION" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="REASON" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="CONTRACTOR" type="ihse_name_code" minOccurs="0"/>
                            <xsd:element name="DEVIATION" type="ihse_decimal" minOccurs="0"/>
                            <xsd:element name="BUILDUP" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="TYPE" type="ihse_name_code" minOccurs="0"/>
                                        <xsd:element name="ANGLE" type="ihse_decimal" minOccurs="0"/>
                                        <xsd:element name="LENGTH" type="ihse_length" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_stratigraphic_unit" minOccurs="0"/>
                            <xsd:element name="LENGTHS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="DISPLACEMENT" type="ihse_distance" minOccurs="0"/>
                                        <xsd:element name="LATERAL" type="ihse_length" minOccurs="0"/>
                                        <xsd:element name="STRATIGRAPHIC_UNIT" type="ihse_length" minOccurs="0"/>
                                        <xsd:element name="PAY" type="ihse_length" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="DEPTHS" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="RAT_HOLE" type="ihse_depth" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="KICKOFF_POINT" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="MEASURED" type="ihse_depth" minOccurs="0"/>
                                                    <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element ref="LOCATION" minOccurs="0"/>
                            <xsd:element name="SPOKE" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="HEADER" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                                    <xsd:element name="LENGTH" type="ihse_length" minOccurs="0"/>
                                                    <xsd:element name="DEPTHS" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="MEASURED" type="ihse_depth" minOccurs="0"/>
                                                                <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element ref="LOCATION" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="POINT_OF_ENTRY" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="HEADER" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="NUMBER" type="xsd:long" minOccurs="0"/>
                                        <xsd:element name="DEPTHS" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="MEASURED" type="ihse_depth" minOccurs="0"/>
                                                    <xsd:element name="TRUE_VERTICAL" type="ihse_depth" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="REMARK" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element ref="LOCATION" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
